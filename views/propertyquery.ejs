<%- include('./partials/adminheader') -%>
<body>
    <div class="container mt-5">
        <table class="table table-striped table-bordered">
            <thead>
              <tr>
                <th scope="col">Query-no</th>
                <th scope="col">queryinfo</th>
                
              </tr>
            </thead>
            <tbody>
              <% for( let  i=0;i<obj.length;i++){ %>
              <tr>
               
                <th scope="row"><%= obj[i].id %>  </th>
                <td><%= obj[i].query %></td>
                
                
              </tr>
              <% } %>
              
            </tbody>
          </table>
    </div>
    <div class="container mt-5">
      <form action="" method="post">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">Query id</label>
          <input type="number" class="form-control" id="exampleInputEmail1" name="queryid" aria-describedby="emailHelp">
        </div>
        <div class="ml-5">
          <button type="submit" class="btn btn-primary m-auto">Submit</button>

        </div>

      </form>
    </div>
   

</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</html>